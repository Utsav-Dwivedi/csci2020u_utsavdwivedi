<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <columns>
            <TableColumn fx:id="ID" text="SID" prefWidth="100"></TableColumn>
            <TableColumn fx:id="assignment" text="Assignments" prefWidth="100"></TableColumn>
            <TableColumn fx:id="midterm" text="Midterm" prefWidth="100"></TableColumn>
            <TableColumn fx:id="exam" text="Final Exam" prefWidth="100"></TableColumn>
            <TableColumn fx:id="finalGrade" text="Final Grade" prefWidth="100"></TableColumn>
            <TableColumn fx:id="letterGrade" text="Letter Grade" prefWidth="100"></TableColumn>

        </columns>
    </TableView>

</GridPane>